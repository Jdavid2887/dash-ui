{
  "name": "dash-ui",
  "theme": "ui",
  "version": "0.4.3",
  "main": "./lib/dash-ui",
  "description": "Matching UI theme for dash-syntax",
  "keywords": [
    "ui",
    "theme",
    "dark"
  ],
  "repository": "https://github.com/cpsdqs/dash-ui",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0 <2.0.0"
  },
  "devDependencies": {
    "cson": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-webfont": "0.0.1-3"
  },
  "configSchema": {
    "themeColor": {
      "title": "Theme Color",
      "order": 1,
      "description": "The UI accent color",
      "type": "string",
      "enum": [
        "Pink",
        "Red",
        "Orange",
        "Yellow",
        "Teal",
        "Green",
        "Cyan",
        "Blue",
        "Indigo",
        "Purple",
        "White"
      ],
      "default": "Pink"
    },
    "iconsEnabled": {
      "title": "File Icons",
      "order": 2,
      "description": "Disable this if you don't want to use the file icons.",
      "type": "boolean",
      "default": true
    }
  }
}
